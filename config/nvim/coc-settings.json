{
  "tsserver.log": "verbose",
  "coc.preferences.formatOnSaveFiletypes": [
    "css",
    "markdown",
    "javascript",
    "typescript",
    "javascriptreact",
    "json",
    "graphql",
    "typescriptreact",
    "scss",
    "html"
